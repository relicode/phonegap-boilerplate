{
  "name": "phonegap-boilerplate",
  "description": "Boilerplate for a PhoneGap project using ES6, React and Redux",
  "author": {
    "name": "Anssi Herranen",
    "url": "https://github.com/relicode",
    "email": "anssi@relicode.fi"
  },
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "reposityory": "https://github.com/relicode/phonegap-boilerplate",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:android": "yarn build && phonegap remote build android",
    "dev": "concurrently \"yarn start\" \"yarn watch\"",
    "lint": "eslint ./src",
    "start": "phonegap serve",
    "watch": "webpack --config webpack.config.js --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.17.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "phonegap": "^6.4.5",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  }
}
